  "components": {
    "schemas": {
      "Application": {
        "required": [
          "bookingId",
          "status",
          "workerId"
        ],
        "type": "object",
        "properties": {
          "applicationId": {
            "type": "integer",
            "format": "int32"
          },
          "bookingId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "workerId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "status": {
            "maxLength": 20,
            "minLength": 0,
            "pattern": "Pending|Approved|Rejected",
            "type": "string"
          },
          "appliedDate": {
            "type": "string",
            "format": "date-time"
          },
          "booking": {
            "$ref": "#/components/schemas/Booking"
          },
          "worker": {
            "$ref": "#/components/schemas/User"
          }
        },
        "additionalProperties": false
      },
      "Booking": {
        "required": [
          "createdAt",
          "customerId",
          "serviceId",
          "status"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "customerId": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "serviceId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "status": {
            "maxLength": 20,
            "minLength": 0,
            "pattern": "Pending|Accepted|Completed|Cancelled",
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "LoginAdminDto": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 8,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Payment": {
        "required": [
          "amount",
          "paymentDate",
          "paymentMethod",
          "userName"
        ],
        "type": "object",
        "properties": {
          "paymentId": {
            "type": "integer",
            "format": "int32"
          },
          "userName": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "amount": {
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "paymentMethod": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "status": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "paymentDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "Rating": {
        "required": [
          "ratingDate",
          "stars",
          "userName",
          "workerId"
        ],
        "type": "object",
        "properties": {
          "ratingId": {
            "type": "integer",
            "format": "int32"
          },
          "workerId": {
            "type": "integer",
            "format": "int32"
          },
          "userName": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "stars": {
            "maximum": 5,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "comment": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "ratingDate": {
            "type": "string",
            "format": "date-time"
          }
        },
